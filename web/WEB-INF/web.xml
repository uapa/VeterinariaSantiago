<?xml version="1.0" encoding="UTF-8"?>
<web-app version="3.0" xmlns="http://java.sun.com/xml/ns/javaee" xmlns:xsi="http://www.w3.org/2001/XMLSchema-instance" xsi:schemaLocation="http://java.sun.com/xml/ns/javaee http://java.sun.com/xml/ns/javaee/web-app_3_0.xsd">
    <welcome-file-list>
        <welcome-file>login.jsp</welcome-file>
    </welcome-file-list>
    <servlet>
        <servlet-name>LoginServlet</servlet-name>
        <servlet-class>org.cfg.uapa.java.controller.LoginServlet</servlet-class>
    </servlet>
    <servlet>
        <servlet-name>RegistroServlet</servlet-name>
        <servlet-class>org.cfg.uapa.java.controller.RegistroServlet</servlet-class>
    </servlet>
    <servlet>
        <servlet-name>DoctorServlet</servlet-name>
        <servlet-class>org.cfg.uapa.java.controller.DoctorServlet</servlet-class>
    </servlet>
    <servlet>
        <servlet-name>EspecieServlet</servlet-name>
        <servlet-class>org.cfg.uapa.java.controller.EspecieServlet</servlet-class>
    </servlet>
    <servlet>
        <servlet-name>RazaServlet</servlet-name>
        <servlet-class>org.cfg.uapa.java.controller.RazaServlet</servlet-class>
    </servlet>
    <servlet>
        <servlet-name>PacienteServlet</servlet-name>
        <servlet-class>org.cfg.uapa.java.controller.PacienteServlet</servlet-class>
    </servlet>
    <servlet>
        <servlet-name>CitaServlet</servlet-name>
        <servlet-class>org.cfg.uapa.java.controller.CitaServlet</servlet-class>
    </servlet>
    <servlet>
        <servlet-name>EditarCitaServlet</servlet-name>
        <servlet-class>org.cfg.uapa.java.controller.EditarCitaServlet</servlet-class>
    </servlet>
    <servlet>
        <servlet-name>BorrarCitaServlet</servlet-name>
        <servlet-class>org.cfg.uapa.java.controller.BorrarCitaServlet</servlet-class>
    </servlet>
    <servlet-mapping>
        <servlet-name>LoginServlet</servlet-name>
        <url-pattern>/LoginServlet</url-pattern>
    </servlet-mapping>
    <servlet-mapping>
        <servlet-name>RegistroServlet</servlet-name>
        <url-pattern>/RegistroServlet</url-pattern>
    </servlet-mapping>
    <servlet-mapping>
        <servlet-name>DoctorServlet</servlet-name>
        <url-pattern>/DoctorServlet</url-pattern>
    </servlet-mapping>
    <servlet-mapping>
        <servlet-name>EspecieServlet</servlet-name>
        <url-pattern>/EspecieServlet</url-pattern>
    </servlet-mapping>
    <servlet-mapping>
        <servlet-name>RazaServlet</servlet-name>
        <url-pattern>/RazaServlet</url-pattern>
    </servlet-mapping>
    <servlet-mapping>
        <servlet-name>PacienteServlet</servlet-name>
        <url-pattern>/PacienteServlet</url-pattern>
    </servlet-mapping>
    <servlet-mapping>
        <servlet-name>CitaServlet</servlet-name>
        <url-pattern>/CitaServlet</url-pattern>
    </servlet-mapping>
    <servlet-mapping>
        <servlet-name>EditarCitaServlet</servlet-name>
        <url-pattern>/EditarCitaServlet</url-pattern>
    </servlet-mapping>
    <servlet-mapping>
        <servlet-name>BorrarCitaServlet</servlet-name>
        <url-pattern>/BorrarCitaServlet</url-pattern>
    </servlet-mapping>
    <session-config>
        <session-timeout>
            30
        </session-timeout>
    </session-config>
</web-app>
